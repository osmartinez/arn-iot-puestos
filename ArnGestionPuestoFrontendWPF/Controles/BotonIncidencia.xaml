<UserControl x:Class="ArnGestionPuestoFrontendWPF.Controles.BotonIncidencia"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ArnGestionPuestoFrontendWPF.Controles"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Button Style="{StaticResource BotonIncidencia}" Margin="1">
        <Image Grid.Row="1"
      
       RenderOptions.BitmapScalingMode="Fant"
       Stretch="Fill"
       Width="100"
       Height="100"
       VerticalAlignment="Top"
       Margin="0,0,0,0"
       HorizontalAlignment="Center" Source="/ArnGestionPuestoFrontendWPF;component/Assets/incidencia.png" />
    </Button>
</UserControl>
